<?xml version="1.0"?>
<robot name="diff_robot">


<material name="black">
    <color rgba="0 0 0 1.0"/>
</material>

<material name="grey">
    <color rgba="0.7 0.7 0.7 1.0"/>
</material>

<material name="green">
    <color rgba="0.0 0.6 0 1.0"/>
</material>

<material name="white">
    <color rgba="1 1 1 1.0"/>
</material>

<link name="base_footprint"/>

<link name="base_link">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.1"/>
        <inertia ixx="0.001665" ixy="0.0" ixz="0.0" iyy="0.00441225" iyz="0.0" izz="0.00541625"/>
    </inertial>
    <visual>
        <geometry>  
            <box size="0.75 0.54 0.2"/> 
        </geometry>   
        <origin xyz="0.0 0.0 0.1" rpy="0.0 0.0 0.0"/> 
        <material name="green"/>
    </visual>
</link>

<link name="left_wheel">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.3"/>
        <inertia ixx="0.00081252" ixy="0.0" ixz="0.0" iyy="0.00081252" iyz="0.0" izz="0.0015"/>
    </inertial>
    <visual>
        <geometry>
            <cylinder radius="0.0625" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="1.57 0.0 0.0"/>
        <material name="black"/>     
    </visual>
    <collision>
        <geometry>
            <cylinder radius="0.0625" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="1.57 0.0 0.0"/>
    </collision>
</link>

<link name="right_wheel">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.3"/>
        <inertia ixx="0.00081252" ixy="0.0" ixz="0.0" iyy="0.00081252" iyz="0.0" izz="0.0015"/>
    </inertial>
    <visual>
        <geometry>
            <cylinder radius="0.0625" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="1.57 0.0 0.0"/>
        <material name="black"/>     
    </visual>
    <collision>
        <geometry>
            <cylinder radius="0.0625" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="1.57 0.0 0.0"/>
    </collision>
</link>

<link name="caster_wheel1">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.1"/>
        <inertia ixx="0.00016666" ixy="0.0" ixz="0.0" iyy="0.00016666" iyz="0.0" izz="0.00016666"/>
    </inertial>
    <visual>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <material name="black"/>            
    </visual>
    <collision>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
</link>

<link name="caster_wheel2">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.1"/>
        <inertia ixx="0.00016666" ixy="0.0" ixz="0.0" iyy="0.00016666" iyz="0.0" izz="0.00016666"/>
    </inertial>
    <visual>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <material name="black"/>            
    </visual>
    <collision>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
</link>

<link name="caster_wheel3">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.1"/>
        <inertia ixx="0.00016666" ixy="0.0" ixz="0.0" iyy="0.00016666" iyz="0.0" izz="0.00016666"/>
    </inertial>
    <visual>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <material name="black"/>            
    </visual>
    <collision>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
</link>

<link name="caster_wheel4">
    <inertial>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <mass value="0.1"/>
        <inertia ixx="0.00016666" ixy="0.0" ixz="0.0" iyy="0.00016666" iyz="0.0" izz="0.00016666"/>
    </inertial>
    <visual>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <material name="black"/>            
    </visual>
    <collision>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
</link>





<link name="imu_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>

  <link name="lidar">
    <visual>
        <geometry>      
            <cylinder radius="0.1" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <material name="white"/>  
    </visual>
    <collision>
        <geometry>
            <cylinder radius="0.05" length="0.05"/>
        </geometry>
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
    </collision>
    <inertial>
        <mass value="0.114" />
        <origin xyz="0 0 0" />
        <inertia ixx="0.001" ixy="0.0" ixz="0.0"
                 iyy="0.001" iyz="0.0"
                 izz="0.001" />
      </inertial>
</link>





<joint name="imu_joint" type="fixed">
    <parent link="base_link"/>
    <child link="imu_link"/>
    <origin xyz="0.1 0.1 0.1" rpy="0.0 0.0 0.0"/>
</joint>

<joint name="base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_footprint"/>
    <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
</joint>

<joint name="base_lidar_joint" type="fixed">
    <parent link="base_link"/>
    <child link="lidar"/>
    <origin xyz="0.0 0.0 0.20" rpy="0.0 0.0 0.0"/>
</joint>

<!-- Fixed Joints -->
<joint name="left_wheel_joint" type="fixed">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <dynamics damping="0.2" friction="0.1"/>
    <origin xyz="0.0 0.270 0.0" rpy="0 0 0"/>  
</joint>

<joint name="right_wheel_joint" type="fixed">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <dynamics damping="0.2" friction="0.1"/>
    <origin xyz="0.0 -0.270 0.0" rpy="0 0 0"/>  
</joint>

<joint name="caster_wheel_joint_Front_right" type="fixed">
    <parent link="base_link"/>
    <child link="caster_wheel1"/>
    <dynamics damping="0.2" friction="0.1"/>
    <origin xyz="0.35 0.2 -0.05" rpy="0.0 0.0 0.0"/>
</joint>

<joint name="caster_wheel_joint_Front_left" type="fixed">
    <parent link="base_link"/>
    <child link="caster_wheel2"/>
    <dynamics damping="0.2" friction="0.1"/>
    <origin xyz="0.35 -0.2 -0.05" rpy="0.0 0.0 0.0"/>
</joint>

<joint name="caster_wheel_joint_Back_right" type="fixed">
    <parent link="base_link"/>
    <child link="caster_wheel3"/>
    <dynamics damping="0.2" friction="0.1"/>
    <origin xyz="-0.35 -0.2 -0.05" rpy="0.0 0.0 0.0"/>
</joint>

<joint name="caster_wheel_joint_Back_left" type="fixed">
    <parent link="base_link"/>
    <child link="caster_wheel4"/>
    <dynamics damping="0.2" friction="0.1"/>
    <origin xyz="-0.35 0.2 -0.05" rpy="0.0 0.0 0.0"/>
</joint> 

<gazebo>
    <plugin name="diff_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <ros>
        <namespace>/</namespace>
        <remapping>cmd_vel:=/cmd_vel</remapping>
        <remapping>odom:=/odom</remapping>
      </ros>
      <left_joint>left_wheel_joint</left_joint>
      <right_joint>right_wheel_joint</right_joint>
      <wheel_separation>0.54</wheel_separation>
      <wheel_radius>0.0625</wheel_radius>
      <odom_frame>odom</odom_frame>
      <robot_base_frame>base_link</robot_base_frame>
      <update_rate>50.0</update_rate>
      <publish_wheel_tf>true</publish_wheel_tf>
      <publish_odom>true</publish_odom>
    </plugin>
  </gazebo>

  <gazebo reference="lidar">
    <sensor type="laser" name="lidar_sensor">
      <pose> 0 0 0 0 0 0 </pose>
      <visualize>true</visualize>    
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>360</samples>
            <resolution>1.0</resolution>
            <min_angle>-1.57</min_angle>
            <max_angle>1.57</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.1</min>
          <max>6.0</max>
          <resolution>0.01</resolution>
        </range>
      </ray>
      <plugin name="gazebo_ros_laser" filename="libgazebo_ros_laser.so">
        <ros>
          <namespace>/</namespace>
          <remapping>scan:=/scan</remapping>
        </ros>
        <frameName>lidar</frameName>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo reference="imu_link">
    <sensor type="imu" name="imu_sensor">
      <update_rate>50.0</update_rate>
      <imu>
        <angular_velocity>true</angular_velocity>
        <linear_acceleration>true</linear_acceleration>
      </imu>
      <plugin name="gazebo_ros_imu" filename="libgazebo_ros_imu.so">
        <ros>
          <namespace>/</namespace>
          <remapping>imu:=/imu</remapping>
        </ros>
        <frameName>imu_link</frameName>
      </plugin>
    </sensor>
  </gazebo>

</robot>
